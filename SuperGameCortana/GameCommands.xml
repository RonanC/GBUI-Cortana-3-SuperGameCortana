<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-gb" Name="GameCommands_en-gb">
    <CommandPrefix>Super Game</CommandPrefix>
    <Example>Start a game of tic tic toe</Example>

    <Command Name="newGame">
      <Example>Start a game of tic tac toe</Example>
      <ListenFor>[start] [a] new game [of] tic tac toe</ListenFor>
      <Feedback>Starting game...</Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>
  </CommandSet>
</VoiceCommands>